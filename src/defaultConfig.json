{
  "dataDirectory": "example",
  "maxZoom": 20,
  "basemaps": [
    {
      "id": "osm",
      "name": "OpenStreetMap Mapnik",
      "url": "https://tile.openstreetmap.org/{z}/{x}/{y}.png",
      "options": {
        "maxNativeZoom": 19,
        "attribution": "&copy; <a href=\"http://www.openstreetmap.org/copyright\">OpenStreetMap</a>"
      }
    }
  ],
  "defaultState": {
    "map": "auto",
    "data": "osm",
    "styleFile": "amenities.yaml"
  }
}
