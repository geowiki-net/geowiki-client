{
  "name": "geowiki-viewer",
  "description": "View custom OSM files with a versatile style file in YAML format.",
  "version": "0.1.0",
  "scripts": {
    "prepare": "npm run build",
    "build": "browserify -p esmify -g browserify-css src/index.js -o dist/map.js",
    "lint": "standard --fix src/*.js",
    "watch": "watchify -p esmify -g browserify-css -d src/index.js -o dist/map.js -v",
    "start": "http-server ./"
  },
  "dependencies": {
    "boundingbox": "^0.1.2",
    "escape-html": "^1.0.3",
    "leaflet": "^1.7.1",
    "leaflet-polylinedecorator": "git+https://github.com/plepe/Leaflet.PolylineDecorator.git",
    "leaflet-polylineoffset": "^1.1.1",
    "leaflet-textpath": "github:makinacorpus/Leaflet.TextPath#leaflet0.8-dev",
    "overpass-frontend": "^3.1.2",
    "overpass-layer": "^3.4.0",
    "yaml": "^1.9.2"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "argparse": "^1.0.10",
    "browserify": "^16.5.2",
    "esmify": "^2.1.1",
    "http-server": "^14.1.1",
    "query-string": "^6.14.1",
    "request": "^2.88.2",
    "sheet-router": "^4.2.3",
    "standard": "^12.0.1",
    "watchify": "^4.0.0",
    "xmldom": "^0.6.0"
  },
  "main": "src/index.js",
  "repository": "https://github.com/geowiki-net/geowiki-viewer",
  "author": "Stephan BÃ¶sch-Plepelits <skunk@xover.mud.at>",
  "license": "GPL-3.0"
}
